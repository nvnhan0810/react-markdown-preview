  "bugs": {
    "url": "https://github.com/nvnhan0810/react-markdown-preview/issues"
  },
  "homepage": "https://github.com/nvnhan0810/react-markdown-preview#readme",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "hast-util-to-jsx-runtime": "^2.3.2",
